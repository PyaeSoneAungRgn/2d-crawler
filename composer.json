{
    "name": "pyaesoneaung/2d-crawler",
    "description": "2D crawler for set.or.th",
    "keywords": [
        "2d-crawler"
    ],
    "homepage": "https://github.com/PyaeSoneAungRgn/2d-crawler",
    "license": "MIT",
    "authors": [
        {
            "name": "Pyae Sone Aung",
            "email": "pyaesoneaungrgn@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/http-client": "^6.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "PyaeSoneAung\\TwoDigitCrawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PyaeSoneAung\\TwoDigitCrawler\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
